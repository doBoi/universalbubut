{
    "version": 2,
      "framework": null,
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
         {
        "src": "/build/assets/(.css)",
        "dest": "/resources/css/"
    },
     {
        "src": "/build/assets/(.js)",
        "dest": "/resources/js/"
    },
         {
        "src": "/images/(.*)",
        "dest": "/public/images/"
    },
        {
        "src": "/(.*)",
        "dest": "/api/index.php"
    }
],
    "env": {
        "APP_ENV": "production",
